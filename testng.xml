<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="OrangeHRM Full Regression Suite">
    
    <listeners>
        <listener class-name="com.qa.listeners.TestListener" />
        <listener class-name="com.qa.utils.AnnotationTransformer" />
    </listeners>

    <test name="OrangeHRM Functional Tests">
    
        <groups>
            <run>
                <exclude name="exclude"/>
            </run>
        </groups>
        
        <classes>
            <class name="com.qa.tests.LoginTest"/>
            <class name="com.qa.tests.AdminTest"/>
            <class name="com.qa.tests.WindowFrameTest"/>
            <class name="com.qa.tests.WindowPaginationTest"/>
        </classes>
    </test>
</suite>